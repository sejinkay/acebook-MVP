
<p><%= session[:current_user_id] %></p>

<% @posts.each do |post| %>
  <p><%= post.message %></p>
  <%= link_to "delete", post, method: :delete,
    data: { confirm: "You sure?" } %>
    <%= link_to "edit", edit_post_path(post) %>
<% end %>
<br/>
<br/>
<%= link_to new_post_path do %>
  New post
<% end %>


<%= link_to logout_path do %>
  Logout
<% end %>
